{
  "name": "aurelia-gridstack-dashboard",
  "description": "Aurelia gridstack dashboard",
  "version": "0.1.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oghanem/aurelia-gridstack-dashboard.git"
  },
  "license": "MIT",
  "keywords": [
    "aurelia",
    "gridstack",
    "webpack"
  ],
  "homepage": "https://github.com/oghanem/aurelia-gridstack-dashboard#readme",
  "scripts": {
    "start": "npm run start:dev",
    "build-release": "rimraf .\\dist && npm run html && npm run css && npm run scss && npm run less && tsc",
    "start:dev": "concurrently --kill-others \"npm run start:webpack-dev-server\"",
    "start:webpack-dev-server": "webpack-dev-server",
    "start:prod": "webpack -p",
    "html": "copyfiles --up 1 src/**/*.html dist/",
    "css": "copyfiles --up 1 src/**/*.css dist/",
    "scss": "copyfiles --up 1 src/**/*.scss dist/",
    "less": "copyfiles --up 1 src/**/*.less dist/",
    "lint": "tsc --noEmit && tslint --project ."
  },
  "dependencies": {
    "aurelia-animator-css": "1.0.4",
    "aurelia-event-aggregator": "^1.0.3",
    "aurelia-framework": "^1.3.1",
    "aurelia-pal": "^1.8.2",
    "aurelia-resize": "^1.1.2",
    "aurelia-router": "1.7.1"
  },
  "devDependencies": {
    "@popperjs/core": "^2.6.0",
    "@types/jest": "^26.0.19",
    "aurelia-bootstrapper": "^2.3.3",
    "aurelia-i18n": "^2.3.2",
    "aurelia-loader-nodejs": "^1.1.0",
    "aurelia-pal-nodejs": "^1.2.0",
    "aurelia-testing": "^1.0.0",
    "aurelia-webpack-plugin": "^4.0.0",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^3.0.0",
    "gridstack": "^3.1.2",
    "html-loader": "^1.3.2",
    "file-loader": "6.2.0",
    "i18next-xhr-backend": "^3.2.2",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-transform-stub": "^2.0.0",
    "less": "^3.13.0",
    "less-loader": "^5.0.0",
    "node-sass": "^7.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^6.0.3",
    "ts-node": "^8.3.0",
    "tslint": "5.18.0",
    "typescript": "^4.1.3",
    "url-loader": "^2.0.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.11.0",
    "worker-loader": "^2.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
